{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-airbnb",
		"tslint-react",
		"tslint-config-prettier",
		"tslint-plugin-prettier"
	],
	"rules": {
		"prettier": true,
		"no-unused-variable": true,
		"import-name": false,
		"object-literal-sort-keys": false,
		"jsx-no-lambda": false,
		"interface-name": [true, "never-prefix"]
	}
}
